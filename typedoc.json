{
	// Comments are supported, like tsconfig.json
	"entryPoints": [
		"apps/client/src/**/*",
		"apps/client/src/App.tsx",
		"apps/server/src/**/*"
	],
	"plugin": [
		"typedoc-plugin-missing-exports",
		"typedoc-plugin-zod",
		"typedoc-plugin-merge-modules"
	],
	"entryPointStrategy": "expand",
	"out": "docs"
}
